<div class="row">
  <div class="large-6 columns"> 
    <!-- Begin MailChimp Signup Form -->
    <link href="//cdn-images.mailchimp.com/embedcode/slim-081711.css" rel="stylesheet" type="text/css">
    <style type="text/css">
    #mc_embed_signup{background:#fff; clear:left; font:14px Helvetica,Arial,sans-serif; }
      /* Add your own MailChimp form style overrides in your site stylesheet or in this style block.
      We recommend moving this block and the preceding CSS link to the HEAD of your HTML file. */
      </style>
      <div id="mc_embed_signup">
        <form action="http://bicyclink.us3.list-manage.com/subscribe/post?u=31610c2d36e1b02ff0149782f&amp;id=5d5b78ab55" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
          <label for="mce-EMAIL">Wanna keep in touch with Bicyclink events and news? </label>
          <input type="email" value="" name="EMAIL" class="email" id="mce-EMAIL" placeholder="email address" required>
          <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
          <div style="position: absolute; left: -5000px;"><input type="text" name="b_31610c2d36e1b02ff0149782f_5d5b78ab55" value=""></div>
          <div class="clear"><input type="submit" value="Subscribe" name="subscribe" id="mc-embedded-subscribe" class="button"></div>
        </form>
      </div>

      <!--End mc_embed_signup--></div>
      <div class="large-6 columns">
        
        <div class="row">
          <div class="large-4 push-8 columns">
           <ul class="social-bar">
            <li>
              <%= link_to "http://www.twitter.com/bicyclinks" do %>
              <i class="fi-social-twitter"></i>
              <%end%>
            </li>

            <li>
              <%= link_to "http://www.facebook.com/bicyclink" do %>
              <i class="fi-social-facebook", color: #8ac007;></i>
              <%end%>
            </li>


            <li><%= link_to "http://www.meetup.com/Bicyclink/" do %>
              <i class="fi-calendar"></i>
              <%end%></li>
            </div>
          </ul>
        </div>
      </div>
    </div>

    



